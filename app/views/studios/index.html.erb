<h1>Listing studios</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Town</th>
      <th>County</th>
      <th>Postcode</th>
      <th>Country</th>
      <th>Website</th>
      <th>Email</th>
      <th>Facebook</th>
      <th>Twitter</th>
      <th>Instagram</th>
      <th>Googleplus</th>
      <th>Googlebusiness</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @studios.each do |studio| %>
      <tr>
        <td><%= studio.name %></td>
        <td><%= studio.address %></td>
        <td><%= studio.town %></td>
        <td><%= studio.county %></td>
        <td><%= studio.postcode %></td>
        <td><%= studio.country %></td>
        <td><%= studio.website %></td>
        <td><%= studio.email %></td>
        <td><%= studio.facebook %></td>
        <td><%= studio.twitter %></td>
        <td><%= studio.instagram %></td>
        <td><%= studio.googleplus %></td>
        <td><%= studio.googlebusiness %></td>
        <td><%= link_to 'Show', studio %></td>
        <td><%= link_to 'Edit', edit_studio_path(studio) %></td>
        <td><%= link_to 'Destroy', studio, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Studio', new_studio_path %>
